replace {
	ld      (hl),a
        dec     hl
} by {
        ld      (hl-),a
}
replace {
	ld	(hl),a
	inc	hl
} by {
	ld	(hl+),a
}
replace {
	ld	a,(hl)
	inc	hl
} by {
	ld	a,(hl+)
}
replace {
	ld	a,[hl]
	inc	hl
} by {
	ld	a,[hl+]
}
replace {
	ld	a,[hl]
	inc	hl
} by {
	ld	a,[hl+]
}
replace {
	ld	[hl],a
	inc	hl
} by {
	ld	[hl+],a
}
replace {
	ld	[hl],a
	dec	hl
} by {
	ld	[hl-],a
}

