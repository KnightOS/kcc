type_name()
{
    if expr "$ac_cv_sizeof_char" '>=' "$1" >/dev/null; then
	echo "char"
	exit
    fi
    if expr "$ac_cv_sizeof_short" '>=' "$1" >/dev/null; then
	echo "short"
	exit
    fi
    if expr "$ac_cv_sizeof_int" '>=' "$1" >/dev/null; then
	echo "int"
	exit
    fi
    if expr "$ac_cv_sizeof_long" '>=' "$1" >/dev/null; then
	echo "long"
	exit
    fi
    if expr "$ac_cv_sizeof_long_long" '>=' "$1" >/dev/null; then
	echo "long long"
	exit
    fi
    echo "long long"
}

echo "$as_me:5028: checking type name for byte" >&5
echo $ECHO_N "checking type name for byte... $ECHO_C" >&6
TYPE_BYTE=`type_name 1`
echo "$as_me:5031: result: $TYPE_BYTE" >&5
echo "${ECHO_T}$TYPE_BYTE" >&6
echo "$as_me:5033: checking type name for word" >&5
echo $ECHO_N "checking type name for word... $ECHO_C" >&6
TYPE_WORD=`type_name 2`
echo "$as_me:5036: result: $TYPE_WORD" >&5
echo "${ECHO_T}$TYPE_WORD" >&6
echo "$as_me:5038: checking type name for dword" >&5
echo $ECHO_N "checking type name for dword... $ECHO_C" >&6
TYPE_DWORD=`type_name 4`
echo "$as_me:5041: result: $TYPE_DWORD" >&5
echo "${ECHO_T}$TYPE_DWORD" >&6
cat >>confdefs.h <<EOF
#define TYPE_BYTE $TYPE_BYTE
EOF

cat >>confdefs.h <<EOF
#define TYPE_WORD $TYPE_WORD
EOF

cat >>confdefs.h <<EOF
#define TYPE_DWORD $TYPE_DWORD
EOF

