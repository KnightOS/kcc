	pula
	psha
	lda	%1,s
} by {
	; Peephole 1b	- removed redundant pula/psha
	lda	%1,s
}

replace {
	pula
	psha
	clra
} by {
	; Peephole 1c	- removed redundant pula/psha
	clra
}


